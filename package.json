{
  "name": "package-json-dependencies-navigation",
  "displayName": "package.json dependencies navigation",
  "description": "Open npm dependencies' page from package.json",
  "version": "0.0.1",
  "publisher": "mamodom",
  "engines": {
    "vscode": "^1.28.0"
  },
  "activationEvents": [
    "onLanguage:json",
    "workspaceContains:**/package.json"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42",
    "@yarnpkg/lockfile": "^1.1.0"
  },
  "license": "SEE LICENSE IN ./LICENSE",
  "categories": [
    "Other"
  ],
  "keywords": [
    "npm",
    "dependencies",
    "link",
    "navigate",
    "dependency"
  ],
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mamodom/package-json-dependencies-navigation"
  }
}
